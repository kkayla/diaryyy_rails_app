<h1>Welcome#index</h1>
<p>Find me in app/views/welcome/index.html.erb</p>

<h1>My Widdle RailsGirls Diary</h1>
  <div>
    <% for entry in @entries %>
      <h2><%= entry.title %></h2>
        <p> <%= entry.date %>
        </p>
        <p>
          <%= entry.contents %>
        </p>
        <% end %>
  </div>

  <% for url, name in @websites %>
    <li><a href="<%= url %>"><%= name %></a></li>
  <% end %>
